# Fog Credentials
#
# Managed by RightScale
# DO NOT EDIT BY HAND
#
# Cloud credentials to be used by Fog
#

# Sort the credential key value pairs for easier inspection
<% creds_array = @creds.to_a %>
<% creds_array.sort! { |elem1, elem2| elem1[0].to_s <=> elem2[0].to_s } %>

:default:
<% creds_array.each do |cred| %>
  :<%=cred[0]%>: "<%=cred[1]%>"
<% end %>
